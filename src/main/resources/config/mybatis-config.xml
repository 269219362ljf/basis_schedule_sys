<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//ibatis.apache.org//DTD Config 3.0//EN" "http://mybatis.apache.org/dtd/mybatis-3-config.dtd">
<configuration>

    <!-- 命名空间 -->
    <typeAliases>
        <!-- 通用类型 -->
        <typeAlias alias="List" type="java.util.List"/>

        <!-- 调度系统 -->
        <typeAlias alias="Task" type="model.Task"/>
        <typeAlias alias="Task_List" type="model.Task_List"/>
        <typeAlias alias="T_Log" type="model.T_Log"/>
        <typeAlias alias="T_param" type="model.T_param"/>

        <!-- 股票分析系统 -->
        <typeAlias alias="T_stock_info" type="stock.T_stock_info"/>
        <typeAlias alias="T_stock_trainning_info" type="stock.T_stock_trainning_info"/>
    </typeAliases>

</configuration>